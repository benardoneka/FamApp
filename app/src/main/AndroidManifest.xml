<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.famappapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FamAppApplication"
        android:usesCleartextTraffic="true">
        <activity android:name=".LiveChat"></activity>
        <activity android:name=".ShareExperience" />
        <activity android:name=".LearnFromCasesActivity" />
        <activity android:name=".TalkToUsActivity" />
        <activity android:name=".AppointmentBookingActivity" />
        <activity android:name=".UpdateFamilyActivity" />
        <activity android:name=".FamilyListActivity" />
        <activity android:name=".AddFamilyActivity" />
        <activity android:name=".Home" />
        <activity android:name=".loginActivity" />
        <activity android:name=".registerActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>